<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.architecturalsample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:icon="@drawable/injection"
        android:label="@string/app_name"
        android:roundIcon="@drawable/injection"
        android:supportsRtl="true"
        android:theme="@style/Theme.ArchitecturalSample">
        <activity android:name=".screens.questiondetails.QuestionDetailsActivity" />
        <activity android:name=".screens.questionslist.QuestionsListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>